{
  "manifest_version": 3,
  "name": "EQ & Volume Boost",
  "version": "1.4.0",
  "default_locale": "en",
  "description": "Powerful and simple volume boost and 3-band equalizer for Chrome.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabCapture",
    "offscreen"
  ]
}